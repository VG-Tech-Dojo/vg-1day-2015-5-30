{
  "$schema": "http://json-schema.org/draft-04/hyper-schema",
  "definitions": {
    "message": {
      "$schema": "http://json-schema.org/draft-04/hyper-schema",
      "title": "メッセージAPI",
      "definitions": {
        "id": {
          "description": "リソース識別用の ID",
          "example": 1192,
          "type": [
            "number"
          ]
        },
        "identity": {
          "$ref": "#/definitions/message/definitions/id"
        },
        "username": {
          "description": "投稿ユーザーの名前",
          "example": "名前はまだ無い",
          "type": [
            "string"
          ]
        },
        "icon": {
          "description": "投稿ユーザーのアイコン画像バイナリを Base64 エンコードしたもの",
          "example": "",
          "type": [
            "string"
          ]
        },
        "body": {
          "description": "メッセージ本文",
          "example": "私はその人を常に先生と呼んでいた。だからここでもただ先生と書くだけで本名は打ち明けない。これは世間を憚る遠慮というよりも、その方が私にとって自然だからである。私はその人の記憶を呼び起こすごとに、すぐ「先生」といいたくなる。筆を執っても心持は同じ事である。よそよそしい頭文字などはとても使う気にならない。",
          "type": [
            "string"
          ]
        },
        "created_at": {
          "description": "メッセージ作成日時 (UTC)",
          "example": "2015-04-23T12:00:00Z",
          "format": "date-time",
          "type": [
            "string"
          ]
        },
        "updated_at": {
          "description": "メッセージ更新日時 (UTC)",
          "example": "2015-04-23T12:00:00Z",
          "format": "date-time",
          "type": [
            "string"
          ]
        }
      },
      "description": "ユーザが投稿したメッセージに関する API",
      "links": [
        {
          "description": "新しくメッセージを作成します。",
          "href": "/messages",
          "method": "POST",
          "rel": "create",
          "schema": {
            "properties": {
              "username": {
                "$ref": "#/definitions/message/definitions/username"
              },
              "body": {
                "$ref": "#/definitions/message/definitions/body"
              }
            },
            "type": [
              "object"
            ],
            "required": [
              "body"
            ]
          },
          "title": "メッセージ作成"
        },
        {
          "description": "指定したメッセージに関する情報を取得します。",
          "href": "/messages/{(%23%2Fdefinitions%2Fmessage%2Fdefinitions%2Fidentity)}",
          "method": "GET",
          "rel": "self",
          "title": "個別のメッセージに関する情報"
        },
        {
          "description": "投稿されたすべてのメッセージを取得します。",
          "href": "/messages",
          "method": "GET",
          "rel": "instances",
          "title": "メッセージ一覧の取得"
        }
      ],
      "properties": {
        "id": {
          "$ref": "#/definitions/message/definitions/id"
        },
        "username": {
          "$ref": "#/definitions/message/definitions/username"
        },
        "icon": {
          "$ref": "#/definitions/message/definitions/icon"
        },
        "body": {
          "$ref": "#/definitions/message/definitions/body"
        },
        "created_at": {
          "$ref": "#/definitions/message/definitions/created_at"
        },
        "updated_at": {
          "$ref": "#/definitions/message/definitions/updated_at"
        }
      },
      "type": [
        "object"
      ]
    }
  },
  "properties": {
    "message": {
      "$ref": "#/definitions/message"
    }
  },
  "type": [
    "object"
  ],
  "links": [
    {
      "href": "http://localhost:8888",
      "rel": "self"
    }
  ]
}
